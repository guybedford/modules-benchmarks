{
  "type": "module",
  "scripts": {
    "build": "mkdir -p results && node generate-cert.js && run-p \"build:*\"",
    "serve-http2": "node static-server/http2.js",
    "serve-https": "node static-server/https.js",
    "serve-http2-cached": "cross-env CACHE=1 node static-server/http2.js",
    "serve-https-cached": "cross-env CACHE=1 node static-server/https.js",
    "serve-all": "run-p serve-http2 serve-https serve-http2-cached serve-https-cached",
    "bench": "run-p serve-all bench-all",
    "bench-all": "run-s \"bench:*\"",
    "build:n-modules": "cd n-modules && npm run build",
    "bench:n-modules": "cd n-modules && npm run bench"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "devcert-sanscache": "^0.4.8",
    "local-web-server": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.14",
    "rollup": "^2.56.3",
    "tachometer": "^0.5.9"
  }
}
