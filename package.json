{
  "type": "module",
  "scripts": {
    "build": "run-p build:es-module-shims generate:chrome generate:cases",
    "build:es-module-shims": "cd es-module-shims && npm install && npm run build && cd ..",
    "generate:cases": "rollup -c rollup.config.js && node generate-cases.js",
    "generate:cert": "node generate-cert.js",
    "generate:chrome": "cross-env BROWSER=chrome node generate-benchmarks.js",
    "generate:safari": "cross-env BROWSER=safari node generate-benchmarks.js",
    "generate:firefox": "cross-env BROWSER=firefox node generate-benchmarks.js",
    "serve": "run-p serve:http2 serve:http2-cached",
    "serve:http2": "node static-server/http2.js",
    "serve:http2-cached": "cross-env CACHE=1 node static-server/http2.js",
    "bench": "run-p serve bench-all",
    "bench-all": "run-s \"bench:*\"",
    "#bench:bundle": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/bundle.bench.json --csv-file-raw ./results/bundle.csv",
    "#bench:bundle-cached": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/bundle-cached.bench.json --csv-file-raw ./results/bundle-cached.csv",
    "#bench:parallel": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel.bench.json --csv-file-raw ./results/parallel.csv",
    "#bench:parallel-cached": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-cached.bench.json --csv-file-raw ./results/parallel-cached.csv",
    "#bench:parallel-mapped": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped.bench.json --csv-file-raw ./results/parallel-mapped.csv",
    "#bench:parallel-mapped-cached": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-cached.bench.json --csv-file-raw ./results/parallel-mapped-cached.csv",
    "#bench:parallel-mapped-esms": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-esms.bench.json --csv-file-raw ./results/parallel-mapped-esms.csv",
    "#bench:parallel-mapped-esms-cached": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-esms-cached.bench.json --csv-file-raw ./results/parallel-mapped-esms-cached.csv",
    "#bench:parallel-mapped-esms-debug": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-esms-debug.bench.json --csv-file-raw ./results/parallel-mapped-esms-debug.csv",
    "#bench:parallel-mapped-esms-debug-cached": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-esms-debug-cached.bench.json --csv-file-raw ./results/parallel-mapped-esms-debug-cached.csv",
    "bench:waterfall": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall.bench.json --csv-file-raw ./results/waterfall.csv",
    "#bench:waterfall-mapped": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall-mapped.bench.json --csv-file-raw ./results/waterfall-mapped.csv",
    "bench:waterfall-mapped-esms": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall-mapped-esms.bench.json --csv-file-raw ./results/waterfall-mapped-esms.csv",
    "bench:waterfall-modulepreload": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall-modulepreload.bench.json --csv-file-raw ./results/waterfall-modulepreload.csv",
    "#bench:waterfall-mapped-modulepreload": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall-mapped-modulepreload.bench.json --csv-file-raw ./results/waterfall-mapped-modulepreload.csv",
    "bench:waterfall-mapped-esms-modulepreload": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/waterfall-mapped-esms-modulepreload.bench.json --csv-file-raw ./results/waterfall-mapped-esms-modulepreload.csv",
    "#bench:parallel-mapped-cdn": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-cdn.bench.json --csv-file-raw ./results/parallel-mapped-cdn.csv",
    "#bench:parallel-mapped-cdn-esms": "node --max-old-space-size=16000 ./node_modules/tachometer/bin/tach --timeout 1 --config benchmarks/parallel-mapped-cdn.bench.json --csv-file-raw ./results/parallel-mapped-cdn-esms.csv"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "devcert-sanscache": "^0.4.8",
    "local-web-server": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.14",
    "rollup": "^2.56.3",
    "tachometer": "^0.5.9"
  }
}
